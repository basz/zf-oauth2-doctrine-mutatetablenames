{
    "name": "bushbaby/zf-oauth2-doctrine-mutatetablenames",
    "description": "Module for Laminas API Tools's Doctrine OAuth2 Server Adapter so table names can be configured",
    "license": "BSD-3-Clause",
    "author": "Bas Kamer",
    "keywords": [
        "oauth2",
        "doctrine",
        "laminas-api-tools"
    ],
    "require": {
        "php": "^7.3 || ~8.0",
        "laminas/laminas-modulemanager": "^2.10.1",
        "laminas/laminas-stdlib": "^2.7 || ^3.0",
        "api-skeletons/oauth2-doctrine": "^4.0 || ^5.1"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\OAuth2\\Doctrine\\MutateTableNames\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "squizlabs/php_codesniffer": "^2.7 || ^3.6",
        "php-coveralls/php-coveralls": "^2.5"
    }
}
